{
  "name": "rewards-cashback-backend",
  "version": "1.0.0",
  "description": "Node.js/Express backend for the Rewards and Cashback application",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "node src/server.js",
    "start": "node src/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:init": "node src/db/migrate.js && node src/db/seed.js",
    "db:migrate": "node src/db/migrate.js",
    "db:seed": "node src/db/seed.js"
  },
  "keywords": [
    "rewards",
    "cashback",
    "express",
    "sqlite"
  ],
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "express-validator": "^7.3.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "supertest": "^7.2.2",
    "vitest": "^4.0.18"
  }
}
